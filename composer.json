{
    "name": "tompedals/helpscout-intercom-app",
    "description": "A Help Scout dynamic app for Intercom",
    "type": "library",
    "license": "MIT",
    "keywords": ["helpscout", "intercom"],
    "require": {
        "php": ">=5.6",
        "intercom/intercom-php": "^3.0",
        "tompedals/helpscout-dynamic-app": "^0.2",
        "symfony/framework-bundle": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "mockery/mockery": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "TomPedals\\HelpScoutApp\\Intercom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomPedals\\HelpScoutApp\\Intercom\\": "tests/"
        }
    }
}
